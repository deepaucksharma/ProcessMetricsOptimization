# Copy this file to .env and provide your personal credentials
NEW_RELIC_LICENSE_KEY=
NEW_RELIC_OTLP_ENDPOINT=https://otlp.nr-data.net/v1/metrics

# Environment settings
OTEL_DEPLOYMENT_ENVIRONMENT=development
OTEL_SERVICE_NAME=otel-collector-host
OTEL_LOG_LEVEL=info

# Collection settings
COLLECTION_INTERVAL=30s
BATCH_SEND_SIZE=1000
BATCH_TIMEOUT=10s

# Memory settings
NEW_RELIC_MEMORY_LIMIT_MIB=250
NEW_RELIC_MEMORY_SPIKE_LIMIT_MIB=150

# Use Docker Compose by default
USE_DOCKER_COMPOSE=true
